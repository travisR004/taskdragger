<ul>
  <% boards.forEach(function(board){ %>
    <li>
      <a href="#/boards/<%= board.escape("id") %>">
      <div class="board">
          <%= board.escape('title')  %>
      </div>
      </a>
    </li>
  <% }) %>
</ul>

<div class="hidden" id="board-form">
  <%= JST['board/new']() %>
</div>

<div id="new-board-button">
<button class="btn btn-primary" id="new-board">New Board</button>
</div>